{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"indent": ["warn", "tab"],
		"linebreak-style": ["warn", "unix"],
		"quotes": ["warn", "single"],
		"semi": ["warn", "never"],
		"react/prop-types": "off",
		"no-unused-vars": "off",
		"react/display-name": "off"
	}
}
